<Window x:Class="Parsing.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Parsing"
        mc:Ignorable="d"
        Title="Parsing" Height="450" Width="500">
    <Grid Background="#1C1C1C">
        <StackPanel Margin="20">
            <Label Content="" Foreground="White" FontSize="25" HorizontalAlignment="Center"/>
            <Separator Background="#FFD700"></Separator>
            <Label Content="Введите имя файла либо путь до него:" FontSize="14" Margin="20 15 20 5" Foreground="White"/>
            <TextBox Name="txtPut" Background="white" Margin="20 5" Foreground="black" FontSize="15"/>
            <Label Margin="20 5" Content="Результат:" FontSize="14" Foreground="White"></Label>
            <TextBox Name="txtResult" Background="white" Margin="20 5" Foreground="black" Height="80" FontSize="15"/>
            <Button Name="btnPasr" Click="Pars_Click"  Content="Выполнить" Margin="150 30" Background="#FFD700" Foreground="black" FontSize="16"/>

        </StackPanel>
    </Grid>
</Window>
